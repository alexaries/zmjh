package com.game.view.fight.begin.tl
{
	public class TL_ALPHA extends BaseTL
	{		
		public var begin_alpha:Number;
		
		public var end_alpha:Number;
		
		public var duration:int;
		
		public function TL_ALPHA()
		{
			super();
		}
		
		override public function parseConfig(data:XML) : void
		{
			super.parseConfig(data);
			
			begin_alpha = data.begin_alpha[0];
			end_alpha = data.end_alpha[0];
			duration = data.duration[0];
		}
	}
}