package com.game.view.union
{
	import starling.display.Image;
	import starling.textures.Texture;
	
	public class MatchPiece extends Image
	{
		

		public var isShow:Boolean = true;
		public function MatchPiece(texture:Texture)
		{
			super(texture);
		}
		
		private var _xPos:int;



		public function get xPos():int
		{
			return _xPos;
		}

		public function set xPos(value:int):void
		{
			_xPos = value;
		}
		
		private var _yPos:int;
		
		public function get yPos():int
		{
			return _yPos;
		}
		
		public function set yPos(value:int):void
		{
			_yPos = value;
		}
		
		private var _type:String;
		
		public function get type():String
		{
			return _type;
		}
		
		public function set type(value:String):void
		{
			_type = value;
		}
		

	}
}