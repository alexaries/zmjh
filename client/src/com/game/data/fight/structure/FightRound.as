package com.game.data.fight.structure
{

	public class FightRound
	{
		/**
		 * 当前回合 
		 */		
		public var Count:int;
		/**
		 * 小回合 
		 */		
		public var SmallCount:int;
		
		/**
		 * 攻击方 
		 */		
		public var attack:AttackRoundData;
		
		/**
		 * 受攻击方 
		 */		
		public var defend:Vector.<DefendRoundData>;
		
		public function FightRound()
		{
			attack = new AttackRoundData();
			defend = new Vector.<DefendRoundData>();
		}
	}
}