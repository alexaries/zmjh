package com.game.controller.player
{
	import com.game.controller.Base;
	import com.game.data.player.structure.Player;

	public class PlayerController extends Base
	{
		public function PlayerController()
		{
		}
		
		public function reqPlayerData(callback:Function) : void
		{
			_controller.data.player.reqPlayerData(callback);
		}
		
		public function getPlayerData() : Player
		{
			return _controller.data.player.player;
		}
	}
}